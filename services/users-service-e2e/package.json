{
  "name": "@task-management/users-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@task-management/users-service"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/users-service-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@task-management/users-service:build",
          "@task-management/users-service:serve"
        ]
      }
    }
  }
}
